#include <stdio.h>
#include <string.h>

typedef struct {
    char name[50];
    char surname[50];
    int year;
} humen;

int main() {
    humen tabl[4], newtabl[4];
    for (int i = 0; i < 4; i++) {
        scanf("%s %s %d", tabl[i].name, tabl[i].surname, &tabl[i].year);
        newtabl[i] = tabl[i];
    }
    for (int i = 0; i < 4; i++) {
        for (int j = i + 1; j < 4; j++) {
            if (newtabl[i].year > newtabl[j].year) {
                humen temp = newtabl[i];
                newtabl[i] = newtabl[j];
                newtabl[j] = temp;
            }
        }
    }
    for (int i = 0; i < 4; i++) {
        printf("%s %s %d\n", newtabl[i].name, newtabl[i].surname, newtabl[i].year);
    }
    return 0;
}